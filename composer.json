{
  "name": "phalcon-boilerplate",
  "description": "A micro framework powered by Phalcon",
  "license": "MIT",
  "minimum-stability":"dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Ardani",
      "email": "master.ardani@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "^5.6.37",
    "illuminate/database": "^5.6.37",
    "illuminate/events": "^5.6.37",
    "league/climate": "^3.4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.3.3",
    "phalcon/incubator": "^3.4",
    "fabfuel/prophiler": "^1.6",
    "symfony/var-dumper": "^4.1.4"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  }
}
